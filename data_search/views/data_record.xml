<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="data_record_view_tree" model="ir.ui.view">
        <field name="name">Data Record List</field>
        <field name="model">data.record</field>
        <field name="arch" type="xml">
            <tree string="Channel" create="0" delete="0" editable="bottom">
                <field name="record_id" invisible="1"/>
                <field name="field" readonly="1"/>
                <field name="model_name" readonly="1"/>
                <field name="model" invisible="1"/>
                <field name="data" readonly="1"/>
                <button type="object" name="action_to_view_data" class="btn btn-secondary fa o_external_button fa-external-link"></button>
            </tree>
        </field>
    </record>
</odoo>